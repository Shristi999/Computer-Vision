import cv2
import numpy
pic=cv2.imread("chris.jpeg")
pic=cv2.resize(pic,(500,500))
pic1=cv2.imread("hames.jpeg")
pic1=cv2.resize(pic1,(500,500))

photo1=numpy.vstack((pic, pic1))
cv2.imshow("vstack",photo1)
if cv2.waitKey()==13:
     pass
cv2.destroyAllWindows()
photo2=numpy.hstack((pic, pic1))
cv2.imshow("hstack",photo2)
if cv2.waitKey()==13:
     pass
cv2.destroyAllWindows()
